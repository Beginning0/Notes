<?xml version="1.0" encoding="utf-8"?>
<android.support.design.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HomeScreen">

    <android.support.design.widget.AppBarLayout
        android:id="@+id/appBarLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:theme="@style/AppTheme.AppBarOverlay">

    </android.support.design.widget.AppBarLayout>


    <android.support.design.widget.FloatingActionButton
        android:id="@+id/atras"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|end"
        android:layout_margin="@dimen/fab_margin"
        app:srcCompat="@android:drawable/ic_input_add" />





    <android.support.v7.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        android:background="?attr/colorPrimary"
        android:minHeight="@dimen/fab_margin"
        app:popupTheme="@style/AppTheme.PopupOverlay" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="match_parent"
        android:layout_height="126dp"
        android:visibility="visible"
        app:layout_anchor="@+id/textView3"
        app:layout_anchorGravity="end|bottom"
        tools:visibility="visible" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="281dp"
        android:layout_height="66dp"
        android:text="* Edgard Henriquez "
        android:textSize="24sp"
        app:layout_anchor="@+id/textView3"
        app:layout_anchorGravity="bottom|center" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="281dp"
        android:layout_height="66dp"
        android:text="* Catalina Ríos Lange"
        android:textSize="24sp"
        app:layout_anchor="@+id/textView4"
        app:layout_anchorGravity="bottom|center" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="337dp"
        android:layout_height="66dp"
        android:text="Acerca de Quien somos"
        android:textSize="30sp"
        app:layout_anchor="@+id/appBarLayout"
        app:layout_anchorGravity="top|center" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="281dp"
        android:layout_height="66dp"
        android:text="* Esteban Terrones S"
        android:textSize="24sp"
        app:layout_anchor="@+id/textView5"
        app:layout_anchorGravity="bottom|center" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="281dp"
        android:layout_height="66dp"
        android:text="* Manuel Cornejo"
        android:textSize="24sp"
        app:layout_anchor="@+id/textView7"
        app:layout_anchorGravity="bottom|center" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="281dp"
        android:layout_height="66dp"
        android:text="* Sebastián Valenzuela"
        android:textSize="24sp"
        app:layout_anchor="@+id/textView8"
        app:layout_anchorGravity="bottom|center" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="66dp"
        android:text="Son los contribuyente para realizar el app Agenda/Notas"
        android:textSize="24sp"
        app:layout_anchor="@+id/textView9"
        app:layout_anchorGravity="bottom|center" />

</android.support.design.widget.CoordinatorLayout>